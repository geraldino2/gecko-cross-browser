{
    "manifest_version": 3,
    "name": "Gecko",
    "description": "Automated CSPT discovery tool",
    "version": "0.1",
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "js/sw.js",
      "type": "module"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ]
  }